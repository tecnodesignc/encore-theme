{
  "name": "tecnodesignc/encore-theme",
  "type": "encore-theme",
  "description": "An EncoreCMS encore bootswatch theme",
  "keywords": [
    "encore-cms"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Marcos Rativa",
      "email": "supoert@tecnodesign.com.co",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "composer/installers": "~1.0"
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan stylist:publish"
    ],
    "post-update-cmd": [
      "php artisan stylist:publish"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
